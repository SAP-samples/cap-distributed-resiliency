              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import org.apache.jmeter.protocol.http.control.CookieManager;
import org.apache.jmeter.protocol.http.control.Cookie;
import java.net.URLEncoder;

def cm = sampler.getCookieManager();
def noOfCookies = cm.getCookieCount();
String host = vars.get(&quot;HTTPRequest_Host&quot;);
	
def newCookies = []
for (def i = 0; i &lt; noOfCookies; i++) {
    Cookie cookie = cm.get(i);
    if (cookie.getName() == &quot;JSESSIONID&quot;) {
        newCookies.add(new Cookie(&quot;JSESSIONID&quot;, cookie.getValue(), &quot;hana.ondemand.com&quot;, &quot;/&quot;, true, 0));
    }
    if (cookie.getName() == &quot;__VCAP_ID__&quot; ) {
        newCookies.add(new Cookie(&quot;__VCAP_ID__&quot;, cookie.getValue(), &quot;hana.ondemand.com&quot;, &quot;/&quot;, true, 0));
    }
}
newCookies.eachWithIndex { item, index -&gt;
  cm.add(item);
}


</stringProp>
