              <stringProp name="script">import org.apache.jmeter.protocol.http.control.CookieManager;
import org.apache.jmeter.protocol.http.control.Cookie;
import java.net.URLEncoder;

String host = vars.get(&quot;HTTPRequest_Host&quot;);
String protocol = vars.get(&quot;HTTPRequest_Protocol&quot;);
String port = vars.get(&quot;HTTPRequest_Port&quot;);
String path= vars.get(&quot;HTTPRequest_Path&quot;);
String query= vars.get(&quot;HTTPRequest_Query&quot;);
String ref =  vars.get(&quot;HTTPRequest_Ref&quot;); 

def cm = sampler.getCookieManager();
def signatureCookie = new Cookie(&quot;signature&quot;, vars.get(&quot;signature&quot;), host, &quot;/&quot;, true, 0);
cm.add(signatureCookie );

if(query) {
	path = path+&quot;?&quot;+query;
}
String locationAfterLogin = URLEncoder.encode(path);
def locationAfterLoginCookie = new Cookie(&quot;locationAfterLogin&quot;, locationAfterLogin, host, &quot;/&quot;, true, 0);
cm.add(locationAfterLoginCookie);

String fragmentAfterLogin = URLEncoder.encode(&quot;#&quot;+ref);


def fragmentAfterLoginCookie = new Cookie(&quot;fragmentAfterLogin&quot;,fragmentAfterLogin, host, &quot;/&quot;, true, 0);
cm.add(fragmentAfterLoginCookie);


</stringProp>
